{
  "css.lint.unknownProperties": "ignore",
  "editor.detectIndentation": false,
  "editor.glyphMargin": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "strings": true,
  },
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/app/build/win-unpacked/**": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/__init__.py": false,
  },
  "git.enabled": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.decorations.enabled": false,
  "git.ignoreLimitWarning": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--disable=W0614"
  ],
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.rulers": [72]
  },
  "[javascript]": {
    "editor.rulers": [
      {
        "column": 80,
      }
    ]
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#0BA67F",
      "iconColour": "green",
      "rulerColour": "green",
      "rulerLane": "left"
    },
    "FIXME": {
      "foreground": "orange",
      "iconColour": "red",
      "rulerColour": "red",
      "rulerLane": "left"
    }
  },
