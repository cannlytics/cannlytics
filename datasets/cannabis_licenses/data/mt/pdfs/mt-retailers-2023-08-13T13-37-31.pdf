	grid-template-columns: 150px 1fr;
	grid-column-gap: 1em;
	border-bottom: 1px solid;
	padding: 1em 0;
	margin: 1em 0;
}
.calendar-event-block * {margin: 0;}
.full-list-title {font-size: 1.1rem;}
.full-list-event-info {position: relative;padding-bottom: 3em;}
.full-list-event-info .view-more {
	display: block;
	position: absolute;
	left: 0;
	bottom: 0;
}

.alert {font-weight: 900; padding-left: 3em;}
.alert::before{
		top: calc(50% - .5em);
	left: calc(1em - 20px);
}
.alert-extension {
	border-width: 0px 3px 3px 3px;
	border-style: solid;
	border-color: var(--dor-primary);
	margin: 0;
	margin-top: -1em;
	padding: 1.2em;
	font-size: 1rem;
	line-height: 1.5em;
	font-family: inherit;
}

.alert-extension ul {margin-left: 50px;}

.alert-newsletter,
.alert-info {
	border-color: var(--dor-primary)!important;}

.alert:not(.alert-info) + .alert-extension {border-color: red;}
.alert:not(.alert-info) + .alert-extension a {color: red;}

.double-block {
	grid-column: span 2;
}

.kudoFor strong {
	font-weight: 900;
	font-size: 1.2rem;
}

.kudo {
	position: relative;
	padding-left: 1em;
	font-style: italic;
	font-size: 1.1rem;
}

.content-wrapper.full-width.with-featured-image {
    border-color: black;
    border-style: solid;
    border-width: 2px;
}

.kudo cite {
	font-style: normal;
	font-size: 1rem;
}

.kudo:before {
	font-family: "Font Awesome 5 Free";
	font-weight: 700;
	font-size: .82rem;
	content: "\f10d";
	position: absolute;
	top: 0;
	left: 0;
}
table {
	max-width: 99%;
	line-height: 1.5;
}

table thead th {
	background-color: var(--dor-primary);
	color: white!important;
}

table tbody th {
	text-align: left;
}

table tbody td {
	vertical-align: middle!important;
}

.dor-button {
	color: white;
	border: 2px solid var(--dor-primary);
	background-color: var(--dor-primary);
	display: inline-block;
	width: auto;
	margin: 0;
	padding: 1em;
	border-radius: 4px;
	box-shadow: 2px 2px 4px var(--dor-dark);
}

.dor-button:hover {
	color: var(--dor-primary);
	border: 2px solid var(--dor-primary);
	background-color: white;
	box-shadow: 2px 2px 4px var(--dor-dark) inset;
}

.ol-two-column {
	margin-left: 5em;
	display: grid;
	grid-template-columns: 1fr 1fr;
	grid-row-gap: 0;
}

.ol-two-column li:before {
	width: 1em;	text-align: right;}

.ol-two-column li {
	grid-column: auto;
}

.five-rows-two-columns {
	max-width: 1200px;
	margin-left: 3em;
	display: grid;
	grid-template-columns: 1fr 1fr;
	grid-template-rows: repeat(5, 1.25em);
	grid-row-gap: 1em;
	grid-auto-flow: column;
}

.five-rows-two-columns li:before {
	width: 1em;	text-align: right;
}

.five-rows-two-columns li {
	grid-column: auto;
}
.announcement {
	display: grid;
	grid-template-columns: 2fr 1fr;
	grid-column-gap: 1em;
	background-color: var(--dor-primary);
	color: white;
	padding: 1em;
	font-weight: 900;
	place-items: center;
}

.announcement div:not(.alert) {
	font-size: 1.2em;
}
.announcement a {
	color: white;
}

.announcement .alert a {
	color: black;
}

.dl-faq {margin: 0;padding: 0;}
.dl-faq dt {font-size: 1.2em;}


.county-list {
	display: grid;
	grid-template-rows: repeat(19, 2em);
	grid-auto-flow: column;
}


@media screen and (max-width: 900px){
		.calculator {
		grid-template-columns: 1fr;
		grid-row-gap: .5em;
	}
	.currencyinput, 
	.calculator input {
		width: 100%;
		margin: 0;
		margin-bottom: 1.2em;

	}
	.row {display: block;}
	
	
.dl-long {
	width: 100%;
	margin: 0;
	display: block;
	columns: 1;
	column-gap: 5vw;
}
.dl-long dt {margin: 0;
	margin-bottom: 1em;}
.dl-long dd {margin-bottom: 1.2em;}

	/* Job Posting Details - Medium screens */
.job-openings-list {
	margin: 1em 0 10px 0;
}

.job-openings-list summary{
	font-size: 1em;
	padding: 1em 2em;
	font-weight: 400;
	text-indent: -1em
	}
	

.job-openings-list[open] summary {
	margin-bottom: 10px;
}


	.job-openings-list .no-collapse-table {
		font-size: 10pt;
	}
	
	
	
	
	.demonstration {
		max-width: 100%;
		font-style: italic;
		margin-bottom: 1.2em;
		line-height: 1.2;
		font-weight: bold;
		text-align: center;
	}
	
	.demonstration figcaption {
		margin: 0 auto;
		margin-bottom: 1.2em;
		max-width: 95%;
	}
	
	.county-list {
		list-style: none;
		place-items: center;
	}
	
	
.alert {
	position: relative;
	display: block;
	grid-template-columns:calc(auto+1em) 1fr;
	font-weight: 900;
	border-radius: 0;
	place-items: center;
	padding-left: 3em;
}

	.alert::before {
		position: absolute;
		left: .25em;
		grid-column: 1/2;
		grid-row: 1/;
	}
	
	
.springboardDetails {
	width: 100%;
	display: grid;
	grid-template-columns: 1fr;
	grid-column-gap: 1em;
	place-items: stretch;
}
	.two-columns {
		grid-template-columns: 1fr;
	}
	.two-columns li {
		margin-bottom: 1em;
	}
	
	table {margin: 0!important;}
		table:not(.no-collapse-table),
		table:not(.no-collapse-table) tbody,
		table:not(.no-collapse-table) thead,
		table:not(.no-collapse-table) tr,
		table:not(.no-collapse-table) th,
		table:not(.no-collapse-table) td{
			display: block;
		}
	
	table:not(.no-collapse-table) {
		border: 0;
	}
	
	table:not(.no-collapse-table) thead tr {
	position: absolute; 
	top: -9999px;
	left: -9999px;
	}

	table:not(.no-collapse-table) tr {
		border: 2px solid var(--dor-dark); 
		margin-bottom: 1em;
	}
	
	table:not(.no-collapse-table) tbody th {
		font-size: .82em;
		background-color: var(--dor-primary);
		color: white;
		text-align: center;
	}
	
	table:not(.no-collapse-table) tbody td {
		border: none; 
		border-bottom: 1px solid;
		position: relative;
		padding-left: 50%;
		min-height: 2em;
	}

	.long {grid-template-columns: 1fr;}
	#Education .long {display: block; columns: 1;}

	
	
